<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.crackers.homebudget">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.AddIncomeActivity"/>
        <activity android:name=".Activities.AddExpences"/>
        <activity android:name=".Activities.TotalBalance"/>
        <activity android:name=".Activities.IncomeList"/>
        <activity android:name=".Activities.ExpencesList"/>
        <activity android:name=".Activities.UpdateIncome"/>
        <activity android:name=".Activities.HelpApp"/>
    </application>

</manifest>